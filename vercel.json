{
    "rewrites": [
        {
            "source": "/obrigado",
            "destination": "/obrigado.html"
        },
        {
            "source": "/api/:match*",
            "destination": "/api/:match*"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}